<Window x:Class="CuentasPorPagar.Views.CRUD.Supplier"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:CuentasPorPagar.Views.CRUD"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:winForms="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        Title="Suplidores"
        Width="434"
        Height="272"
        MinWidth="574"
        MinHeight="332"
        MaxWidth="574"
        MaxHeight="332"
        Loaded="Window_Loaded"
        mc:Ignorable="d">
    <Grid Margin="0,0,0,29">
        <Button x:Name="DeleteSupplierBtn"
                Width="76"
                Height="20"
                Margin="144,265,0,-13"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="DeleteSupplierBtn_Click"
                Content="Eliminar"
                FontWeight="Bold"
                IsEnabled="False" />
        <Button x:Name="CreateSupplierBtn"
                Width="75"
                Height="20"
                Margin="50,265,0,-13"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="CreateSupplierBtn_Click"
                Content="Guardar"
                FontWeight="Bold"
                RenderTransformOrigin="0.533,2.25" />
        <Button x:Name="ExitSupplierBtn"
                Width="74"
                Height="20"
                Margin="465,260,0,-8"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="ExitSupplierBtn_Click"
                Content="Salir"
                FontWeight="Bold" />
        <DataGrid x:Name="SupplierDgv"
                  Width="515"
                  Height="170"
                  Margin="30,75,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="100,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="ID" HorizontalAlignment="Left" Margin="40,15,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="100,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1" Content="Nombre" HorizontalAlignment="Left" Margin="40,40,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="350,13,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label2" Content="Tipo" HorizontalAlignment="Left" Margin="290,15,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" Margin="350,45,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label3" HorizontalAlignment="Left" Margin="290,44,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button"  HorizontalAlignment="Left" Margin="220,45,0,0" VerticalAlignment="Top" Width="25" RenderTransformOrigin="0.5,0.75" Height="20">
            <Image x:Name="image3" Height="15" Width="15" Source="/CuentasPorPagar;component/Resources/lupa.png" Stretch="Fill"/>
        </Button>
    </Grid>
</Window>
