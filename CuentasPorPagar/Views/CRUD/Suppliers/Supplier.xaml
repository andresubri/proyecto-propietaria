<Window x:Class="CuentasPorPagar.Views.CRUD.Supplier"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:CuentasPorPagar.Views.CRUD"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:winForms="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        Title="Suplidor"
        Width="574"
        Height="467"
        mc:Ignorable="d" Loaded="Window_Loaded">
    <Grid Margin="0,0,32,169">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="230*"/>
            <ColumnDefinition Width="281*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="115*"/>
            <RowDefinition Height="154*"/>
        </Grid.RowDefinitions>
        <DataGrid x:Name="SupplierDgv"
                  Width="520"
                  Height="212"
                  Margin="25,28,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top" Grid.ColumnSpan="2" Grid.RowSpan="2" />
        <Button x:Name="EditSupplierBtn"
                Width="76"
                Margin="174,146,0,-13"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Editar" Grid.Row="1" Click="EditSupplierBtn_Click" Grid.ColumnSpan="2" />
        <Button x:Name="DeleteSupplierBtn"
                Width="76"
                Margin="24,146,0,-13"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Eliminar" Grid.Row="1" Click="DeleteSupplierBtn_Click" Grid.Column="1" />
        <Button x:Name="CreateSupplierBtn"
                Width="75"
                Margin="75,146,0,-13"
                HorizontalAlignment="Left"
                Content="Crear" Click="CreateSupplierBtn_Click" Height="20" VerticalAlignment="Top" Grid.Row="1" RenderTransformOrigin="0.533,2.25" />
        <Button x:Name="ExitSupplierBtn"
                Width="75"
                Margin="110,146,0,-13"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Salir" Grid.Row="1" Click="ExitSupplierBtn_Click" Grid.Column="1" />
    </Grid>
</Window>
