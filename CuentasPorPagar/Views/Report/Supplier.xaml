<Window x:Class="CuentasPorPagar.Views.Report.Supplier"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        xmlns:local="clr-namespace:CuentasPorPagar.Views.Report"
        mc:Ignorable="d"
        Title="Supplier" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="165*"/>
            <ColumnDefinition Width="127*"/>
        </Grid.ColumnDefinitions>
        <WindowsFormsHost HorizontalAlignment="Left" Height="240" Margin="16,15,0,0" VerticalAlignment="Top" Width="261" Grid.ColumnSpan="2" Loaded="WindowsFormsHost_Loaded">
            <rv:ReportViewer x:Name="_reportViewer"/>
        </WindowsFormsHost>

    </Grid>
</Window>
